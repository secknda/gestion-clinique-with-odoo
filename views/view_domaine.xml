<?xml version="1.0" encoding="utf-8"?>
 <odoo>
 	<data>

 		<record id="gestion_clinique_domaine_view_tree" model="ir.ui.view">
 			<field name="name">gestion_clinique_domaine_view_tree</field>
			<field name="model">gestion.clinique.domaine</field>
			<field name="arch" type="xml">
			    <tree string="Domaine">
			      <field name="name" />
			      <field name="code" />
			    </tree>
			</field>
 		</record>

 		<record id="gestion_clinique_domaine_view_form" model="ir.ui.view">
 			<field name="name">gestion_clinique_domaine_view_form</field>
 			<field name="model">gestion.clinique.domaine</field>
 			<field name="arch" type="xml">
			    <form string="Domaine">
			    	<group>
			    		<field name="name" />
					    <field name="code" />
			    	</group>
			    </form>
			</field>
 		</record>

 		<record model="ir.actions.act_window" id="action_gestion_clinique_domaine">
 			<field name="name">Domaine</field>
		    <field name="type">ir.actions.act_window</field>
		    <field name="res_model">gestion.clinique.domaine</field>
		    <field name="view_mode">tree,form</field>	
 		</record>


 	</data>
 </odoo>
